{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "class-methods-use-this" : "off",
        "no-unneeded-ternary":"off",
        "prefer-destructuring":"off",
        "func-names":"off",
        "prefer-template":"off",
        "no-underscore-dangle":"off"
    },
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
}
